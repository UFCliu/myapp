<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.liugq.mapper.QiShiMapper" >
  <resultMap id="BaseResultMap" type="com.example.liugq.bean.dzm.QiShi" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="name" property="name" jdbcType="VARCHAR" />
    <result column="property" property="property" jdbcType="VARCHAR" />
    <result column="color" property="color" jdbcType="VARCHAR" />
    <result column="first_level" property="firstLevel" jdbcType="VARCHAR" />
    <result column="second_level" property="secondLevel" jdbcType="VARCHAR" />
    <result column="third_level" property="thirdLevel" jdbcType="VARCHAR" />
    <result column="fourth_level" property="fourthLevel" jdbcType="VARCHAR" />
    <result column="fifth_level" property="fifthLevel" jdbcType="VARCHAR" />
    <result column="remark" property="remark" jdbcType="VARCHAR" />
  </resultMap>
  <sql id="Base_Column_List" >
    id, name, property, color, first_level, second_level, third_level, fourth_level, fifth_level, 
    remark
  </sql>
  
	<!-- 真气数据查询 -->
  	<select id="qiShiQuery" resultMap="BaseResultMap" >
    	select 
   		<include refid="Base_Column_List" />
     		from dzm_qishi where 1=1 
    	<if test="params.property != null">
			and property = #{params.property}	
		</if>
  	</select>
</mapper>